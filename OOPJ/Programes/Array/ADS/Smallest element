class SmallestNum{
    public static void main(String args[]){
        int []a={24,75,21,11,18};
        int min=a[0];
        
        for(int i=0;i<a.length;i++){
            if(a[i]<min)
                min=a[i];
        }
        System.out.println("Smallest number in array element= "+min);
    }
}
