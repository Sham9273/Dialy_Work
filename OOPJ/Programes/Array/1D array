import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
        int []arr={10,20,30};
        
    // To string method to display array element
        
        //System.out.println(Arrays.toString(arr));
        
    // To display array element using for loop
    
        for(int i=0;i<arr.length;i++){
            System.out.println(arr[i]);
        }
    }
}
